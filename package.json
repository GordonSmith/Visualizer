{
  "private": true,
  "name": "@hpcc-js/visualizer",
  "version": "2.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf lib* types dist res/*.js res/*.map",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "terser dist/index.js -c -m -o res/index.min.js",
    "build": "run-s compile-es6 bundle minimize",
    "watch": "run-p compile-es6-watch bundle-watch",
    "dev-link": "rimraf ./node_modules/@hpcc-js && mklink /J .\\node_modules\\@hpcc-js ..\\hpcc-js\\packages",
    "dev-unlink": "rimraf ./node_modules/@hpcc-js && npm install",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "devDependencies": {
    "d3-fetch": "1.1.2",
    "@hpcc-js/api": "2.7.7",
    "@hpcc-js/chart": "2.17.1",
    "@hpcc-js/codemirror": "2.8.6",
    "@hpcc-js/codemirror-shim": "2.12.1",
    "@hpcc-js/common": "2.16.7",
    "@hpcc-js/comms": "2.6.7",
    "@hpcc-js/composite": "2.4.7",
    "@hpcc-js/dgrid": "2.4.7",
    "@hpcc-js/dgrid-shim": "2.10.0",
    "@hpcc-js/form": "2.7.7",
    "@hpcc-js/graph": "2.9.7",
    "@hpcc-js/layout": "2.12.7",
    "@hpcc-js/loader": "2.6.7",
    "@hpcc-js/map": "2.7.7",
    "@hpcc-js/other": "2.12.7",
    "@hpcc-js/phosphor": "2.12.1",
    "@hpcc-js/phosphor-shim": "2.10.1",
    "@hpcc-js/timeline": "2.6.7",
    "@hpcc-js/tree": "2.7.7",
    "@hpcc-js/util": "2.6.7",
    "d3-geo": "1.6.4",
    "d3-color": "1.2.3",
    "d3-scale": "3.0.0",
    "@deck.gl/core": "7.0.4",
    "@deck.gl/layers": "7.0.4",
    "mapbox-gl": "0.54.0",
    "leaflet": "1.4.0",
    "leaflet.gridlayer.googlemutant": "GordonSmith/Leaflet.GridLayer.GoogleMutant#master",
    "leaflet.markercluster": "1.4.1",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "rollup": "1.10.1",
    "rollup-plugin-alias": "1.5.1",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-postcss": "2.0.3",
    "tslib": "1.9.3",
    "tslint": "5.16.0",
    "typescript": "3.4.4",
    "terser": "3.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GordonSmith/Visualizer.git"
